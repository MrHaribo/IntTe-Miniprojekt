<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>link</managed-bean-name>
		<managed-bean-class>rClone.hsr.ch.Link</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>linklist</managed-bean-name>
		<managed-bean-class>rClone.hsr.ch.Linklist</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>user</managed-bean-name>
		<managed-bean-class>rClone.hsr.ch.User</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>userMap</managed-bean-name>
		<managed-bean-class>rClone.hsr.ch.UserMap</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>linksubmit</managed-bean-name>
		<managed-bean-class>rClone.hsr.ch.Linksubmit</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>linksubmit.xhtml</display-name>
		<from-view-id>/linksubmit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>linksubmit</from-outcome>
			<to-view-id>/linklist.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>linksubmit.xhtml</display-name>
		<from-view-id>/linksubmit.xhtml</from-view-id>
		<navigation-case>
			<to-view-id>/register.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
